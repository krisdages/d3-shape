{
  "name": "@krisdages/d3-shape",
  "version": "3.1.0",
  "description": "Graphical primitives for visualization, such as lines and areas.",
  "homepage": "https://d3js.org/d3-shape/",
  "repository": {
    "type": "git",
    "url": "https://github.com/d3/d3-shape.git"
  },
  "keywords": [
    "d3",
    "d3-module",
    "graphics",
    "visualization",
    "canvas",
    "svg"
  ],
  "license": "ISC",
  "author": {
    "name": "Mike Bostock",
    "url": "http://bost.ocks.org/mike"
  },
  "type": "module",
  "files": [
    "dist",
    "src/**/*.js"
  ],
  "module": "src/index.js",
  "main": "dist/d3-shape.min.cjs",
  "jsdelivr": "dist/d3-shape.min.js",
  "unpkg": "dist/d3-shape.min.js",
  "exports": {
    "umd": "./dist/d3-shape.min.js",
    "require": "./dist/d3-shape.min.cjs",
    "default": "./src/index.js"
  },
  "sideEffects": false,
  "dependencies": {
    "d3-path": "npm:@krisdages/d3-path@<4"
  },
  "devDependencies": {
    "d3-polygon": "1 - 3",
    "eslint": "8",
    "mocha": "9",
    "rollup": "2",
    "rollup-plugin-terser": "7"
  },
  "scripts": {
    "test": "mocha 'test/**/*-test.js' && eslint src test",
    "prepublishOnly": "rm -rf dist && yarn test && rollup -c"
  },
  "engines": {
    "node": ">=12"
  }
}
